    private String status;
    private int department;
    private String purpose;
    private long startTime;
    private int center;
    private String title;
    private boolean isVisitMeeting;
    private int userId;
    private int meetingId;
    private boolean isRecuringMeeting;
    private String meetingUId;
    private String organizerEmail;
    private String room;

    public String getStatus() {
        return status;
    }

    public void setStatus(String status) {
        this.status = status;
    }

    public int getDepartment() {
        return department;
    }

    public void setDepartment(int department) {
        this.department = department;
    }

    public String getPurpose() {
        return purpose;
    }

    public void setPurpose(String purpose) {
        this.purpose = purpose;
    }

    public long getStartTime() {
        return startTime;
    }

    public void setStartTime(long startTime) {
        this.startTime = startTime;
    }

    public int getCenter() {
        return center;
    }

    public void setCenter(int center) {
        this.center = center;
    }

    public String getTitle() {
        return title;
    }

    public void setTitle(String title) {
        this.title = title;
    }

    public boolean isVisitMeeting() {
        return isVisitMeeting;
    }

    public void setVisitMeeting(boolean visitMeeting) {
        isVisitMeeting = visitMeeting;
    }

    public int getUserId() {
        return userId;
    }

    public void setUserId(int userId) {
        this.userId = userId;
    }

    public int getMeetingId() {
        return meetingId;
    }

    public void setMeetingId(int meetingId) {
        this.meetingId = meetingId;
    }

    public boolean isRecuringMeeting() {
        return isRecuringMeeting;
    }

    public void setRecuringMeeting(boolean recuringMeeting) {
        isRecuringMeeting = recuringMeeting;
    }

    public String getMeetingUId() {
        return meetingUId;
    }

    public void setMeetingUId(String meetingUId) {
        this.meetingUId = meetingUId;
    }

    public String getOrganizerEmail() {
        return organizerEmail;
    }

    public void setOrganizerEmail(String organizerEmail) {
        this.organizerEmail = organizerEmail;
    }

    public String getRoom() {
        return room;
    }

    public void setRoom(String room) {
        this.room = room;
    }

    @Override
    public String toString() {
        return "MeetingDetails{" +
                "status='" + status + '\'' +
                ", department=" + department +
                ", purpose='" + purpose + '\'' +
                ", startTime=" + startTime +
                ", center=" + center +
                ", title='" + title + '\'' +
                ", isVisitMeeting=" + isVisitMeeting +
                ", userId=" + userId +
                ", meetingId=" + meetingId +
                ", isRecuringMeeting=" + isRecuringMeeting +
                ", meetingUId='" + meetingUId + '\'' +
                ", organizerEmail='" + organizerEmail + '\'' +
                ", room='" + room + '\'' +
